{
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    "C_Cpp.errorSquiggles": "Disabled",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**.exe": true
    },
    "files.associations": {
        "*.tcc": "cpp",
        "initializer_list": "cpp",
        "utility": "cpp",
        "cmath": "cpp",
        "cstdlib": "cpp",
        "limits": "cpp",
        "iostream": "cpp",
        "ostream": "cpp"
    }
}